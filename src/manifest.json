{
    "name": "KeyCuts",
    "description": "A Chrome Extension that makes web navigation simple, easy, and convenient for its users.",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage","tabs"],
    "options_page": "options.html",
    "omnibox": {
      "keyword": "!"
    },
    "icons":{
      "16":"img/KC.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }